{
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "dist/**"
            ]
        },
        "tauri-build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "release/**"
            ]
        },
        "dev": {
            "dependsOn": [
                "^dev"
            ]
        },
        "tauri-dev": {
            "dependsOn": [
                "^build"
            ]
        },
        "turbo": {
            "dependsOn": [
                "^build"
            ]
        },
        "start": {
            "dependsOn": [
                "build"
            ]
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "dependsOn": [
                "^build"
            ]
        }
    }
}